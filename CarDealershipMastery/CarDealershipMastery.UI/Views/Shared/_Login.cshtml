@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    <ul class="nav navbar-nav navbar-right">
        <li>
            <a href="#">Hello @User.Identity.GetUserName()!</a>
        </li>
        <li>
            @Html.ActionLink("Logout", "Logout", "Account")
        </li>
    </ul>
}
