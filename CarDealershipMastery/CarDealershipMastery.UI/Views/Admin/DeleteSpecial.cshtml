@model CarDealershipMastery.Models.TableModels.Special
@{
    ViewBag.Title = "DeleteSpecial";
}

<h2>DeleteSpecial</h2>
@using (@Html.BeginForm("DeleteSpecial", "Admin", FormMethod.Post))
{
    @Html.HiddenFor(m => m.SpecialId)
    <p>Are you sure you want to delete post @Model.SpecialTitle?</p>
    <button type="submit" class="btn btn-danger">Delete</button>
    @Html.ActionLink("Cancel", "Specials", "Admin", null, new { @class = "btn btn-info" })
}


