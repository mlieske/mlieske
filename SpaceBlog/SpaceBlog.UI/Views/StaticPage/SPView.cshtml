@model SpaceBlog.Models.StaticPage
@{
    ViewBag.Title = "SPView";
}

<style>
    body{
        color:white;
    }
</style>
<h2>SPView</h2>
<div class="row">
    <div class="col-xs-12">
        <div class="row">
            <div class="col-xs-8">

                <label></label>
                <div>
                    @Model.StaticPageTitle
                </div><br />
                <label></label>
                <div>
                    @Html.Raw(Model.StaticPageBody)
                </div><br />
            </div>
            <div class="col-xs-3 col-xs-offset-1">
                <img src="~/Website IMAGES/@Model.ImageFileName" width="200" />
            </div>
        </div>
    </div>
</div>


